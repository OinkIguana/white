            <string>///Create, move, and destroy flakes
if(ds_exists(flakes, ds_type_list) &amp;&amp; ds_exists(speeds, ds_type_list)) {
    ds_list_insert(flakes, 0, irandom(room_width));
    ds_list_insert(speeds, 0, 90);
    for(var i = 0; i &lt; room_height; i++) {
        flakes[| i] -= sin(degtorad(speeds[| i]));
        if(speeds[| i] != 90) {
            speeds[| i] = (speeds[| i] + 6) % 360;
