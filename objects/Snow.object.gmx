            if(!irandom(128)) {
                speeds[| i] = 96;
            }
        }
    }
    while(ds_list_size(flakes) &gt; room_height) {
        ds_list_delete(flakes, ds_list_size(flakes) - 1);
        ds_list_delete(speeds, ds_list_size(speeds) - 1);
    }
}
